 (
   Username VARCHAR(10)   Primary Key,
   Password VARCHAR(10)   UNIQUE NOT NULL,    
   FName	VARCHAR(15),
   MName    VARCHAR(15),
   LName    VARCHAR(15),
   Street   VARCHAR(20),
   City     VARCHAR(15),
   State    VARCHAR(15),
   ZIP      INTEGER,
 );

